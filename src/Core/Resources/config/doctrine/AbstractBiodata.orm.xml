<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <mapped-superclass name="Paroki\Core\Entity\AbstractBiodata" >
    <field name="nama" type="string" length="255" nullable="false">
      <options>
        <option name="default">N/A</option>
      </options>
    </field>
    <field name="jenisKelamin" type="integer" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tempatLahir" type="string" length="255" nullable="true"/>
    <field name="tanggalLahir" type="date" nullable="true"/>
    <field name="ayah" type="string" length="255" nullable="true"/>
    <field name="ibu" type="string" length="255" nullable="true"/>
    <field name="alamat" type="string" length="255" nullable="true"/>
  </mapped-superclass>
</doctrine-mapping>

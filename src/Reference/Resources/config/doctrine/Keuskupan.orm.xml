<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SIAP\Reference\Entity\Keuskupan" table="ref_keuskupan">
    <id name="id" type="guid">
      <generator strategy="UUID"/>
    </id>
    <field name="kode" type="string" unique="true" length="3" nullable="false" column="kode"/>
    <field name="noUrutKeuskupan" type="integer" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="namaKeuskupan" type="string" length="100" nullable="true"/>
    <field name="namaKeuskupanLatin" type="string" length="50" nullable="true"/>
    <field name="alamatKeuskupan" type="string" length="150" nullable="true"/>
    <field name="kotaKeuskupan" type="string" length="50" nullable="true"/>
    <field name="teleponKeuskupan" type="string" length="50" nullable="true"/>
    <field name="faxKeuskupan" type="string" length="50" nullable="true"/>
    <field name="websiteKeuskupan" type="string" length="50" nullable="true"/>
    <field name="emailKeuskupan" type="string" length="50" nullable="true"/>
    <field name="namaUskupSaatIni" type="string" length="50" nullable="true"/>
    <unique-constraints>
      <unique-constraint name="ix_keuskupan_kode" columns="kode"/>
    </unique-constraints>
  </entity>
</doctrine-mapping>

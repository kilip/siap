<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Paroki\Reference\Entity\Lingkungan" table="ref_lingkungan">
    <id name="id" type="string" length="4" column="id"/>
    <field name="lingkunganId" type="string" unique="true" length="75" nullable="true"/>
    <field name="noLingkungan" type="integer" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="namaKetuaLingkungan" type="string" length="255" nullable="true"/>
    <field name="f5" type="string" length="255" nullable="true"/>
    <many-to-one field="wilayah" target-entity="Paroki\Reference\Entity\Wilayah" inversed-by="lingkungans">
      <join-columns>
        <join-column name="wilayah_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="bagian" target-entity="Paroki\Reference\Entity\WilayahBagian">
      <join-columns>
        <join-column name="wilayah_bagian_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

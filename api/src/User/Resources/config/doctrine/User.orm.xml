<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SIAP\User\Entity\User" extends="FOS\UserBundle\Model\User" table="sc_user">
    <id name="id" type="guid">
      <generator strategy="UUID"/>
    </id>
    <field name="nama" type="string" nullable="true"/>
    <many-to-one field="paroki" target-entity="SIAP\Reference\Entity\Paroki" fetch="EAGER">
      <join-columns>
        <join-column name="paroki_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

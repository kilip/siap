<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SIAP\Komuni\Entity\Komuni">
    <id name="id" type="guid">
      <generator strategy="CUSTOM"/>
      <custom-id-generator class="Ramsey\Uuid\Doctrine\UuidGenerator"/>
    </id>
    <field name="buku" type="string" nullable="true"/>
    <field name="halaman" type="integer" nullable="true"/>
    <field name="nomor" type="integer" nullable="true"/>
    <field name="tanggal" type="date" nullable="true"/>
    <field name="gelarPemberi" type="string" nullable="true"/>
    <field name="yangMemberi" type="string" nullable="true"/>
    <field name="parokiAsal" type="string" nullable="true"/>
    <field name="namaBaptis" type="string" nullable="true"/>
    <field name="wali1" type="string" nullable="true"/>
    <field name="wali2" type="string" nullable="true"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SIAP\Core\Entity\Dokumen" table="td_core_dokumen">
    <id name="id" type="guid">
      <generator strategy="UUID"/>
    </id>
    <field name="nama" type="string" nullable="false"/>
    <field name="uploadedAt" type="date" nullable="true"/>
    <field name="fileLocation" type="string" length="255" nullable="true"/>
    <many-to-one field="uploadedBy" target-entity="SIAP\User\Entity\User">
      <join-columns>
        <join-column name="uploaded_by_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
